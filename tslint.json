{
  "extends": ["tslint:latest", "tslint-blueprint"],
  "rules": {
    "no-console": false,
    "eofline": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "quotemark": false,
    "ordered-imports": false,
    "no-empty": false,
    "no-duplicate-imports": false,
    "no-submodule-imports": false
  }
}